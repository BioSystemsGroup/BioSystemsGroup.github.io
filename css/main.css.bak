body {	margin: 0px;	padding: 0px;	background-color: #FFFFFF;	font-family: Arial, Helvetica, sans-serif;	background-image: url(../images/background.gif);	background-repeat: repeat-y;}#header {	background-color: #FFFFFF;	width: 940px;}td, tr, table {	vertical-align: top;}p {	font-size: 85%;	line-height: 140%;	margin-bottom: 18px;	margin-right: 8px;	margin-left: 8px;}p.breadcrumb {	font-size: 70%;}p.small {    font-size: 50%;}blockquote {	font-size: 70%;	line-height: 140%;	margin-bottom: 18px;	margin-right: 28px;	margin-left: 28px;}h1 {	font-size: 18px;	margin-bottom: 14px;	color: #003399;	padding:6px 0px 10px 0px;	margin-right: 8px;	margin-left: 8px;}h2 {	font-size: 16px;	margin-bottom: 15px;	border-bottom: 1px solid #BBE5EF;	color: #003399;	margin-right: 8px;	margin-left: 8px;	padding-top: 10px;}h3 {	font-size: 14px;	margin-top: 5px;	color: #003399;	margin-right: 8px;	margin-left: 8px;}h3 a{	color: #003399;}ol li { font-size: 85%; margin-bottom: 5px;}ul li{	font-size: 85%;	list-style-image:url(../images/bullet.gif);	padding: 5px;}a {	text-decoration: none;}a.reference {  color: #000;}a:hover {	text-decoration: underline;}.active-link:hover {	text-decoration: none;}.header_img {	margin-bottom: 10px;	}.navigation {	padding-top: 0px;}.left-box {	background-color: #F0F9FB;	position: relative;	width: 177px;	height: 100%;}.left-top {	position: absolute;	top: 0px;	left: 0px;	width: 153px;	height: 24px;	border-top: 3px solid #BBE5EF;	background-color: #F0F9FB;}.pop-box {	background-color: #F0F9FB;	margin: 10px;	width: 90%;	padding: 10px;	border: 3px solid #BBE5EF;}/* ignored by ie */html > body .left-top {	width: 156px;}.left-corner {	background: url("../images/corner_right.gif") no-repeat top right; 	position: absolute;	top: 0px;	left: 153px;	width: 24px;	height: 24px;}/* ignored by ie */html > body .left-corner {	left: 156px;}.left-content {	background-color: #F0F9FB;	position: absolute;	top: 24px;	left: 0px;	width: 177px;	height: 100%;	border-right: 3px solid #BBE5EF;}.nav-link {	padding: 8px 0px 10px 0px;	border-bottom: 3px solid #BBE5EF;	background-color: #F0F9FB;	width: 170px;	margin-top: 10px;	margin-bottom: 10px;}#top-nav-link {	margin-top: -8px;}.nav-link a {	font-weight: bold;	font-size: 16px;	color: #1BA9CB;	font-family: Arial, Helvetica, sans-serif;	margin-left: 6px;}a.active-link{	color: #003399;}.news {	padding-top: 3px;}.right-box {	position: relative;	background-color: #F0F9FB;		width: 237px;	height: 100%;}.right-corner {	background: url("../images/corner_left.gif") no-repeat top left; 	position: absolute;	top: 0px;	left: 0px;	width: 24px;	height: 24px;}.right-top {	position: absolute;	top: 0px;	left: 24px;	width: 216px;	height: 24px;	border-top: 3px solid #BBE5EF;	background-color: #F0F9FB;}#top-news {	margin-top: 0px;}.right-content {	background-color: #F0F9FB;	position: absolute;	top: 24px;	left: 0px;	width: 237px;	height: 100%;	border-left: 3px solid #BBE5EF;}.sample {	margin-top: 4px;}.news-link {	margin: 20px 8px 0px 8px;	line-height: 100%;}